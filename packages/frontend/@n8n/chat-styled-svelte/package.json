{
  "name": "@n8n/chat-styled-svelte",
  "version": "0.1.0",
  "description": "Professional AI chat UI built on @n8n/chat-headless-svelte with modern design",
  "scripts": {
    "dev": "vite",
    "build": "vite build && pnpm build:types",
    "build:types": "tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "test": "vitest run",
    "test:dev": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --quiet",
    "lint:fix": "eslint src --fix"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/chat-styled-svelte.umd.js",
  "module": "./dist/chat-styled-svelte.es.js",
  "svelte": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/chat-styled-svelte.es.js",
      "require": "./dist/chat-styled-svelte.umd.js"
    },
    "./styles.css": {
      "import": "./dist/styles.css",
      "require": "./dist/styles.css"
    },
    "./*": {
      "import": "./*",
      "require": "./*"
    }
  },
  "dependencies": {
    "@n8n/chat-headless-svelte": "workspace:*"
  },
  "devDependencies": {
    "@n8n/eslint-config": "workspace:*",
    "@n8n/typescript-config": "workspace:*",
    "@n8n/vitest-config": "workspace:*",
    "@sveltejs/vite-plugin-svelte": "^5.0.4",
    "@tsconfig/svelte": "^5.0.4",
    "@vitest/coverage-v8": "3.2.4",
    "svelte": "^5.23.1",
    "typescript": "5.9.2",
    "vite": "npm:rolldown-vite@latest",
    "vitest": "^3.1.3"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "files": [
    "README.md",
    "dist"
  ]
}
